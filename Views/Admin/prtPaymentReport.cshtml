@model IEnumerable<ShopingCart.Models.PaymentReport><style>    table {        width: 100%;        border-collapse: collapse;        border: 2px solid black;    }    #tblorder th {        background-color: black;        text-align: center;        color: white;        font-weight: bold;    }    td, th {        padding: 10px;        text-align: center;        border: 1px solid #ddd;    }</style><table id="tblorder" class="table table-bordered">    <thead>        <tr>            <th>Payment Id</th>            <th>Order Id</th>            <th>Amount</th>            <th>Entry Date</th>        </tr>    </thead>    <tbody>        @foreach (var item in Model)
        {            <tr>                <td>@item.PaymentId</td>                <td>@item.OrderId</td>                <td>@item.Amount</td>                <td>@item.EntryDate</td>            </tr>        }    </tbody></table>